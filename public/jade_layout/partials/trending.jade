
div.col-md-6
  table(class="table") 
    tr.trendtable(ng-repeat="trending in trendings | limitTo:5")
      td.pic
        img(src="{{trending.data.thumbnail}}" ng-click='trendart(trending.data.url)')
      td.trend
        h2 {{trending.data.title}}
